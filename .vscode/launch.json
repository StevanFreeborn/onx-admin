      "launchConfigurationId": "TargetFramework=;https"
    },
    {
      "name": "OnxAdmin.Client",
      "request": "launch",
      "type": "msedge",
      "webRoot": "${workspaceFolder}/src/OnxAdmin.Client",
      "url": "https://localhost:4000"
    },
  ]
}