{
  "compounds": [
    {
      "name": "OnxAdmin",
      "configurations": [
        "OnxAdmin.AppHost",
        "OnxAdmin.Client"
      ]
    }
  ],
  "configurations": [
    {
      "name": "OnxAdmin.AppHost",
      "type": "dotnet",
      "request": "launch",
